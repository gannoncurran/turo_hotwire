{
  "name": "turo_code_challenge",
  "version": "0.0.1",
  "description": "A universal app that uses the HotWire API to search for rental cars",
  "repository": {
    "type": "git",
    "url": "https://github.com/gannoncurran/turo_hotwire"
  },
  "main": "./src/client/index.jsx",
  "engines": {
    "node": "=4.4.3",
    "npm": "=2.15.1"
  },
  "private": true,
  "scripts": {
    "test": "mocha config/mocha/setup.js \"src/**/*.spec.js\" --reporter progress",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "start": "npm run start:app",
    "start:app": "node ./src/server/server.js",
    "production": "npm run build && NODE_ENV=production node ./src/server/server.js",
    "build": "rimraf public && NODE_ENV=production webpack --config ./webpack.build.config.js --progress --profile --colors"
  },
  "keywords": [
    "express",
    "webpack",
    "react",
    "redux",
    "universal",
    "hotwire"
  ],
  "author": "gannoncurran",
  "license": "ISC",
