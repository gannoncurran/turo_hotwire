{
  "name": "express_webpack_react",
  "version": "0.0.1",
  "description": "Express, Webpack, React Skeleton with dev and production server setup",
  "repository": {
    "type": "git",
    "url": "https://github.com/HelloElephant/express_webpack_react"
  },
  "main": "index.jsx",
  "engines": {
    "node": "=4.4.3",
    "npm": "=2.15.1"
  },
  "private": true,
  "scripts": {
    "test": "mocha config/mocha/setup.js \"src/**/*.spec.js\" --reporter progress",
    "test:watch": "npm run test -- --watch --watch-extensions jsx",
    "start": "node server.js",
    "production": "npm run build && NODE_ENV=production node server.js",
    "build": "npm test && rimraf public && NODE_ENV=production webpack --config ./webpack.build.config.js --progress --profile --colors"
  },
  "keywords": [
    "express",
    "webpack",
    "react",
    "production"
  ],
  "author": "gannoncurran",
  "license": "ISC",
