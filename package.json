    "debug": "^2.2.0",
    "express": "^4.13.4",
    "helmet": "^2.1.1",
    "lodash.template": "^4.2.5",
    "morgan": "^1.7.0",
    "request": "^2.72.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "axios": "^0.11.1",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
